<?xml version="1.0" encoding="UTF-8"?>
<svg width="800px" height="600px" viewBox="0 0 800 600" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <title>PC방 이미지</title>
    <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
        <rect fill="#0A192F" x="0" y="0" width="800" height="600"></rect>
        <g transform="translate(100, 50)">
            <!-- PC방 배경 -->
            <rect fill="#0D2038" x="0" y="0" width="600" height="400" rx="20" stroke="#0055FF" stroke-width="4"></rect>
            
            <!-- 책상 -->
            <rect fill="#1C2C3C" x="50" y="200" width="500" height="30" rx="5"></rect>
            
            <!-- PC 1 -->
            <rect fill="#0F1824" x="100" y="100" width="120" height="80" rx="5" stroke="#0055FF" stroke-width="2"></rect>
            <rect fill="#FFFFFF" x="110" y="110" width="100" height="60" rx="3"></rect>
            <rect fill="#1C2C3C" x="130" y="230" width="60" height="50" rx="5"></rect>
            
            <!-- PC 2 -->
            <rect fill="#0F1824" x="240" y="100" width="120" height="80" rx="5" stroke="#0055FF" stroke-width="2"></rect>
            <rect fill="#FFFFFF" x="250" y="110" width="100" height="60" rx="3"></rect>
            <rect fill="#1C2C3C" x="270" y="230" width="60" height="50" rx="5"></rect>
            
            <!-- PC 3 -->
            <rect fill="#0F1824" x="380" y="100" width="120" height="80" rx="5" stroke="#0055FF" stroke-width="2"></rect>
            <rect fill="#FFFFFF" x="390" y="110" width="100" height="60" rx="3"></rect>
            <rect fill="#1C2C3C" x="410" y="230" width="60" height="50" rx="5"></rect>
            
            <!-- EdgeX 장비 -->
            <rect fill="#001133" x="100" y="300" width="400" height="60" rx="10" stroke="#0055FF" stroke-width="3"></rect>
            <text x="190" y="340" font-family="Arial" font-size="24" fill="#FFFFFF">EdgeX XR7 Devices</text>
            <circle cx="150" cy="330" r="15" fill="#0055FF"></circle>
            <circle cx="450" cy="330" r="15" fill="#0055FF"></circle>
            
            <!-- 네트워크 라인 -->
            <path d="M150,330 L150,280 L270,280 L270,230" stroke="#0055FF" stroke-width="2" stroke-dasharray="5,5"></path>
            <path d="M450,330 L450,280 L410,280 L410,230" stroke="#0055FF" stroke-width="2" stroke-dasharray="5,5"></path>
            <path d="M300,330 L300,280 L270,280" stroke="#0055FF" stroke-width="2" stroke-dasharray="5,5"></path>
            
            <!-- 수익 기호 -->
            <circle cx="550" cy="50" r="30" fill="rgba(0,85,255,0.2)" stroke="#0055FF" stroke-width="2"></circle>
            <text x="538" y="58" font-family="Arial" font-size="24" fill="#FFFFFF">$</text>
            <path d="M550,50 L600,70" stroke="#0055FF" stroke-width="2" stroke-dasharray="5,5"></path>
            
            <!-- 코인/마이닝 심볼 -->
            <circle cx="50" cy="50" r="30" fill="rgba(0,85,255,0.2)" stroke="#0055FF" stroke-width="2"></circle>
            <text x="38" y="58" font-family="Arial" font-size="20" fill="#FFFFFF">₿</text>
            <path d="M50,50 L0,70" stroke="#0055FF" stroke-width="2" stroke-dasharray="5,5"></path>
        </g>
        
        <!-- 문구 추가 -->
        <text x="400" y="500" font-family="Arial" font-size="30" fill="#FFFFFF" text-anchor="middle">EdgeX PC방 파트너십</text>
        <text x="400" y="540" font-family="Arial" font-size="20" fill="#0055FF" text-anchor="middle">추가 수익 창출의 기회</text>
    </g>
</svg>
